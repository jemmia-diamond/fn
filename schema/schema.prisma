generator client {
  provider        = "prisma-client-js"
  output          = "../../node_modules/@prisma-cli"
  previewFeatures = ["driverAdapters", "multiSchema", "queryCompiler"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["ecom", "erpnext", "inventory_cms", "larksuite", "misa", "payment"]
}
