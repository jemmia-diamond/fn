{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2020",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "es2020"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ],
    "strictNullChecks": true
  }
}