model purchase_exchange_policy {
  order_id    String  @db.VarChar(50)
  item_id     String  @db.VarChar(50)
  order_code  String? @db.VarChar(50)
  sku         String? @db.VarChar(50)
  item_name   String? @db.VarChar(255)
  barcode     String? @db.VarChar(50)
  policy_id   String? @db.VarChar(50)
  policy_name String? @db.VarChar

  @@id([order_id, item_id])
  @@schema("policy")
}
