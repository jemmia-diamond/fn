  report_dt                  String?   @db.VarChar(255)
  shape                      String?   @db.VarChar(255)
  measurements               String?   @db.VarChar(255)
  weight                     String?   @db.VarChar(255)
  color_grade                String?   @db.VarChar(255)
  clarity_grade              String?   @db.VarChar(255)
  cut_grade                  String?   @db.VarChar(255)
  depth                      String?   @db.VarChar(255)
  table_size                 String?   @db.VarChar(255)
  crown_angle                String?   @db.VarChar(255)
  crown_height               String?   @db.VarChar(255)
  pavilion_angle             String?   @db.VarChar(255)
  pavilion_depth             String?   @db.VarChar(255)
  star_length                String?   @db.VarChar(255)
  lower_half                 String?   @db.VarChar(255)
  girdle                     String?   @db.VarChar(255)
  culet                      String?   @db.VarChar(255)
  polish                     String?   @db.VarChar(255)
