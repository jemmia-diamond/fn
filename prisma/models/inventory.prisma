  staff          Decimal?  @db.Decimal
  count_in_book  Decimal?  @db.Decimal
  count_for_real Decimal?  @db.Decimal
  extra          Decimal?  @db.Decimal
  lines          Json?
  created_at     DateTime? @default(now()) @db.Timestamp(6)
  updated_at     DateTime? @default(now()) @db.Timestamp(6)
