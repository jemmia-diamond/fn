model manualPaymentTransaction {
  uuid                String    @id @default(uuid()) @db.Uuid
  payment_type        String?   @db.VarChar(255)
  branch              String?   @db.VarChar(255)
  shipping_code       String?   @db.VarChar(255)
  send_date           DateTime? @db.Timestamp(6)
  receive_date        DateTime? @db.Timestamp(6)
  created_date        DateTime? @db.Timestamp(6)
  updated_date        DateTime? @db.Timestamp(6)
  bank_account        String?   @db.VarChar(255)
  bank_name           String?   @db.VarChar(255)
  transfer_amount     Decimal?  @db.Decimal(18, 6)
  transfer_note       String?   @db.Text
  haravan_order_id    String?   @db.VarChar(255)
  haravan_order_name  String?   @db.VarChar(255)
  transfer_status     String?   @db.VarChar(255)

  lark_record_id      String?   @unique @db.VarChar(255)

  misa_synced         Boolean   @default(false)
  misa_sync_guid      String?   @db.VarChar(255)
  misa_sync_error_msg String?   @db.Text

  @@index([uuid])
  @@map("manual_payments")
  @@schema("payment")
}
