model ecom_leads {
  id                  Int       @id @default(autoincrement())
  raw_data            Json?
  database_created_at DateTime? @default(now()) @db.Timestamp(6)
  custom_uuid         String?   @unique @default(dbgenerated("(gen_random_uuid())::text"))

  @@map("leads")
  @@schema("ecom")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model products {
  haravan_product_id        BigInt?   @unique(map: "products_unique_haravan_id")
  haravan_product_type      String?
  design_id                 BigInt?
  handle                    String?   @db.VarChar
  workplace_id              Int?
  category                  String?
  title                     String?
  min_price                 Decimal?  @db.Decimal
  max_price                 Decimal?  @db.Decimal
  qty_onhand                BigInt?
  image_updated_at          DateTime? @db.Timestamp(6)
  wedding_ring_id           Int?
  primary_collection        String?
  primary_collection_handle String?
  pages                     String?
  max_price_18              Int?
  max_price_14              Int?

  @@ignore
  @@schema("ecom")
}

model qrPaymentTransaction {
  id                        String    @id @db.VarChar
  bank_code                 String?   @db.VarChar
  bank_account_number       String?   @db.VarChar
  customer_name             String?   @db.VarChar
  customer_phone_number     String?   @db.VarChar
  transfer_amount           BigInt?
  transfer_note             String?   @db.VarChar
  transfer_status           String?   @db.VarChar
  haravan_order_number      String?   @db.VarChar
  haravan_order_status      String?   @db.VarChar
  haravan_order_id          Int?
  haravan_order_total_price BigInt?
  created_at                DateTime? @default(now()) @db.Timestamp(6)
  updated_at                DateTime? @default(now()) @db.Timestamp(6)
  deleted_at                DateTime? @db.Timestamp(6)
  is_deleted                Boolean?
  qr_url                    String?
  lark_record_id            String?   @db.VarChar(255)
  all_lark_record_id        String?   @db.VarChar(255)
  misa_synced               Boolean   @default(false)
  misa_synced_at            DateTime? @db.Timestamp(6)
  misa_sync_guid            String?   @db.VarChar(255)
  misa_sync_error_msg       String?
  haravan_order Order? @relation(fields: [haravan_order_id], references: [id])
  
  @@index([id], map: "ix_ecom_qr_generator_id")
  @@map("qr_generator")
  @@schema("ecom")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model variants {
  hararvan_product_id BigInt?
  haravan_variant_id  BigInt?  @unique(map: "unique_haravan_variant_id")
  sku                 String?  @db.VarChar
  price               Decimal? @db.Decimal
  price_compare_at    Decimal? @db.Decimal(36, 8)
  material_color      String?
  fineness            String?
  ring_size           Decimal? @db.Decimal
  haravan_product_id  Int?

  @@ignore
  @@schema("ecom")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model wedding_rings {
  id               Int?      @unique
  title            String?
  max_price        Decimal?  @db.Decimal
  min_price        Decimal?  @db.Decimal
  image_updated_at DateTime? @db.Timestamp(6)

  @@ignore
  @@schema("ecom")
}
