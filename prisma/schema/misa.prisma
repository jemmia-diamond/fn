model misaUser {
    uuid                String @id @default(uuid()) @db.Uuid
    employee_code       String? @unique @db.VarChar(255)
    haravan_id          BigInt @unique @db.BigInt
    email               String? @unique @db.VarChar(255)
    database_created_at DateTime? @default(now())
    database_updated_at DateTime? @updatedAt

    @@index([haravan_id])
    @@index([email])
    @@map("users")
    @@schema("misa")
}
