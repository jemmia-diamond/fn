model misaUser {
  uuid         String    @id @default(uuid()) @db.Uuid
  name         String?   @unique
  email        String?
  birth_date   DateTime?
  creation     DateTime? @db.Timestamp(6)
  enabled      Int?      @db.SmallInt
  full_name    String?
  gender       String?
  language     String?
  location     String?
  modified     DateTime? @db.Timestamp(6)
  modified_by  String?
  pancake_id   String?
  role_profile String?
  time_zone    String?
  user_image   String?

  @@map("users")
  @@schema("erpnext")
}

