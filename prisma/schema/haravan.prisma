model Customer {
  uuid                  String    @id @db.VarChar
  id                    Int?      @unique(map: "ix_haravan_customers_id")
  accepts_marketing     Boolean?
  default_address       Json?
  addresses             Json?
  address_address1      String?   @db.VarChar
  address_address2      String?   @db.VarChar
  address_city          String?   @db.VarChar
  address_company       String?   @db.VarChar
  address_country       String?   @db.VarChar
  address_country_code  String?   @db.VarChar
  address_id            BigInt?
  address_first_name    String?   @db.VarChar
  address_last_name     String?   @db.VarChar
  address_phone         String?   @db.VarChar
  address_province      String?   @db.VarChar
  address_province_code String?   @db.VarChar
  address_zip           String?   @db.VarChar
  address_name          String?   @db.VarChar
  address_default       Boolean?
  address_district      String?   @db.VarChar
  address_district_code String?   @db.VarChar
  address_ward          String?   @db.VarChar
  address_ward_code     String?   @db.VarChar
  email                 String?   @db.VarChar
  phone                 String?   @db.VarChar
  first_name            String?   @db.VarChar
  last_name             String?   @db.VarChar
  created_at            DateTime? @db.Timestamp(6)
  updated_at            DateTime? @db.Timestamp(6)
  multipass_identifier  Boolean?
  last_order_id         BigInt?
  last_order_name       String?   @db.VarChar
  published             Boolean?
  note                  String?   @db.VarChar
  orders_count          Int?
  state                 String?   @db.VarChar
  tags                  String?   @db.VarChar
  total_spent           Decimal?  @db.Decimal(36, 8)
  total_paid            Decimal?  @db.Decimal(36, 8)
  verified_email        Boolean?
  group_name            String?   @db.VarChar
  birthday              DateTime? @db.Timestamp(6)
  gender                Int?
  database_created_at   DateTime? @default(now()) @db.Timestamp(6)
  database_updated_at   DateTime? @default(now()) @db.Timestamp(6)

  @@index([uuid], map: "ix_haravan_customers_uuid")
  @@map("customers")
  @@schema("haravan")
}

