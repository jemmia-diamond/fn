generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

generator zod {
  provider = "prisma-zod-generator"
  output   = "./generated/zod"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["advertising_cost", "bizflycrm", "conversation_rate", "crm_dashboard", "dashboard_reporting", "data_2024", "ecom", "ecommerce", "erpnext", "gia", "haravan", "inventory", "inventory_cms", "inventory_report", "jemmia", "larksuite", "market_data", "misa", "pancake", "payment", "policy", "promotion", "rapnet", "reporting", "salesaya", "supplychain", "workplace"]
}

