generator client {
  provider        = "prisma-client-js"
  output          = "../../node_modules/@prisma-cli"
  previewFeatures = ["driverAdapters", "multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["erpnext", "larksuite", "inventory_cms"]
}
