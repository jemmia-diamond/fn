
model jewelryDiamondPairing {
  id                         String   @unique @default(dbgenerated("(gen_random_uuid())::text"))
  haravan_product_id         Int
  haravan_variant_id         Int
  haravan_diamond_product_id Int
  haravan_diamond_variant_id Int
  is_active                  Boolean  @default(true)
  created_at                 DateTime @default(now())
  updated_at                 DateTime @default(now())

  @@unique([haravan_product_id, haravan_variant_id, haravan_diamond_product_id, haravan_diamond_variant_id], map: "jewelry_diamond_pairing_unique_pairing")
  @@index([id])
  @@index([haravan_product_id, haravan_variant_id, is_active])
  @@index([haravan_diamond_product_id, haravan_diamond_variant_id])
  @@map("jewelry_diamond_pairs")
  @@schema("workplace")
}

