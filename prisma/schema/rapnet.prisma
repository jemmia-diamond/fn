model diamonds_dev {
  diamond_id     String    @id @db.VarChar
  gia_report_no  String?   @db.VarChar
  price          Decimal?  @db.Decimal
  country        String?   @db.VarChar
  is_available   Boolean?
  created_at     DateTime? @db.Timestamp(6)
  updated_at     DateTime? @db.Timestamp(6)
  gia_info       Json?
  sent_event     Json?
  shade          String?   @db.VarChar
  key_to_symbols String?   @db.VarChar
  inclusions     String?   @db.VarChar
  diamond_data   Json?

  @@schema("rapnet")
}

model diamonds_prod {
  diamond_id     String    @id @db.VarChar
  gia_report_no  String?   @db.VarChar
  price          Decimal?  @db.Decimal
  country        String?   @db.VarChar
  is_available   Boolean?
  created_at     DateTime? @db.Timestamp(6)
  updated_at     DateTime? @db.Timestamp(6)
  gia_info       Json?
  sent_event     Json?
  shade          String?   @db.VarChar
  key_to_symbols String?   @db.VarChar
  inclusions     String?   @db.VarChar
  diamond_data   Json?

  @@schema("rapnet")
}
