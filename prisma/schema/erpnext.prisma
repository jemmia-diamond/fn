  total                          Decimal? @db.Decimal(18, 6)
  discount_amount                Decimal? @db.Decimal(18, 6)
  grand_total                    Decimal? @db.Decimal(18, 6)
  base_total                     Decimal? @db.Decimal(18, 6)
  base_net_total                 Decimal? @db.Decimal(18, 6)
  total_net_weight               Decimal? @db.Decimal(18, 6)
  net_total                      Decimal? @db.Decimal(18, 6)
  
  // Tax information
  tax_category                String?
  taxes_and_charges           String?
  shipping_rule               String?
  incoterm                    String?
  named_place                 String?
  base_total_taxes_and_charges Decimal? @db.Decimal(18, 6)
  total_taxes_and_charges     Decimal? @db.Decimal(18, 6)
  base_grand_total            Decimal? @db.Decimal(18, 6)
  base_rounding_adjustment    Decimal? @db.Decimal(18, 6)
  base_rounded_total          Decimal? @db.Decimal(18, 6)
  base_in_words               String?
  rounding_adjustment         Decimal? @db.Decimal(18, 6)
  rounded_total               Decimal? @db.Decimal(18, 6)
  in_words                    String?
  advance_paid                Decimal? @db.Decimal(18, 6)
  disable_rounded_total       Int?
  other_charges_calculation   String?
  
  // Contact and address information
  contact_person          String?
  contact_display         String?
  contact_phone           String?
  contact_mobile          String?
  contact_email           String?
  customer_address        String?
  address_display         String? @db.Text
  customer_group          String?
  territory               String?
  shipping_address_name   String?
  shipping_address        String?
  customer                String?
  gender                  String?
  customer_type           String?
  customer_personal_id    String?
  birth_date              DateTime?
  date_of_issuance        DateTime?
  dispatch_address        String?
  place_of_issuance       String?
  dispatch_address_name   String?
  company_address         String?
  company_address_display String?
  company_contact_person  String?
  
  // Status information
  status          String?
  delivery_status String?
  per_delivered   Decimal? @db.Decimal(18, 6)
  per_billed      Decimal? @db.Decimal(18, 6)
  per_picked      Decimal? @db.Decimal(18, 6)
  billing_status  String?
  
  // Sales and commission
  sales_partner                   String?
  amount_eligible_for_commission  Decimal? @db.Decimal(18, 6)
  commission_rate                 Decimal? @db.Decimal(18, 6)
  total_commission                Decimal? @db.Decimal(18, 6)
  loyalty_points                  Int?
  loyalty_amount                  Decimal? @db.Decimal(18, 6)
  from_date                       DateTime?
  to_date                         DateTime?
  auto_repeat                     String?
  
  // Print and display
  letter_head           String?
  group_same_items      Int?
  select_print_heading  String?
  language              String?
  is_internal_customer  Int?
  represents_company    String?
  source                String?
  inter_company_order_reference String?
  campaign              String?
  party_account_currency String?
  
  // Payment information
  total_amount            Decimal? @db.Decimal(18, 6)
  expected_payment_date   DateTime?
  paid_amount             Decimal? @db.Decimal(18, 6)
  balance                 Decimal? @db.Decimal(18, 6)
  payment_terms_template  String?
  tc_name                 String?
  terms                   String? @db.Text
  
  // Haravan integration
  haravan_order_id        String?
  haravan_ref_order_id    String?
  haravan_created_at      DateTime?
  source_name             String?

  // JSONB fields
  sales_team          Json? @db.JsonB
  ref_sales_orders    Json? @db.JsonB
  promotions          Json? @db.JsonB
  product_categories  Json? @db.JsonB
  packed_items        Json? @db.JsonB
  taxes               Json? @db.JsonB
  pricing_rules       Json? @db.JsonB
  payment_records     Json? @db.JsonB
  payment_schedule    Json? @db.JsonB
  policies            Json? @db.JsonB
  items               Json? @db.JsonB

  database_created_at     DateTime? @default(now()) @db.Timestamp(6)
  database_updated_at     DateTime? @updatedAt @db.Timestamp(6)

  @@map("sales_orders")
  @@schema("erpnext")
}

