  cost_center                                 String?   @db.VarChar(140)
  parent                                      String?   @db.VarChar(140)
  parentfield                                 String?   @db.VarChar(140)
  parenttype                                  String?   @db.VarChar(140)
  doctype                                     String?   @db.VarChar(140)
  database_created_at                         DateTime? @default(now()) @db.Timestamp(6)
  database_updated_at                         DateTime? @db.Timestamp(6)

  @@index([barcode])
  @@index([haravan_variant_id])
  @@index([item_name])
  @@index([parent])
  @@index([sku])
  @@map("sales_order_items")
  @@schema("erpnext")
}

