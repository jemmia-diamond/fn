model ERPNextUser {
  uuid                String @id @default(uuid()) @db.Uuid
  name                String? @unique
  email               String?

  @@map("users")
  @@schema("erpnext2")
}

model ERPNextEmployee {
  uuid                String @id @default(uuid()) @db.Uuid
  name                String? @unique
  user_id             String?

  @@map("employee")
  @@schema("erpnext2")
}


model ERPNextSalesPerson {
  uuid                String @id @default(uuid()) @db.Uuid
  name                String? @unique
  employee            String?

  @@map("sales_persons")
  @@schema("erpnext2")
}